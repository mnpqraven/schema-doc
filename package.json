{
  "private": true,
  "scripts": {
    "build": "dotenv -c -- turbo run build --no-daemon",
    "dev": "dotenv -c -- turbo run dev --parallel",
    "db:push": "dotenv -c -- turbo run db:push",
    "codegen": "turbo run codegen",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "autoprefixer": "^10.4.16",
    "eslint": "^8.53.0",
    "next": "^14.0.2",
    "postcss": "^8.4.31",
    "prettier": "^3.1.0",
    "tailwindcss": "^3.3.5",
    "tsconfig": "workspace:*",
    "turbo": "^1.10.16"
  },
  "packageManager": "pnpm@8.6.10"
}
